{
  "name": "daily-waterfall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "commit": "git cz",
    "code-submit": "yarn setgit && git add . && git cz && git push && yarn unsetgit",
    "setgit": "git config --global user.name \"cycleyzc\" && git config --global user.email \"cycle900802@gmail.com\"",
    "unsetgit": "git config --global --unset user.name && git config --global user.email --unset"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vue-select": "^2.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "better-scroll": "^1.15.1",
    "cz-conventional-changelog": "^2.1.0",
    "dexie": "^2.0.4",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-saver": "^2.0.1",
    "husky": "^2.3.0",
    "icon-font-loader": "^2.0.3",
    "izitoast": "^1.4.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.4",
    "postcss-px-to-viewport": "^1.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.1",
    "vue-awesome-swiper": "^3.1.3",
    "vue-notifications": "^0.9.0",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.5.21",
    "vue2-datepicker": "^2.10.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": ["plugin:vue/essential", "@vue/standard"],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": ["vue-cli-service lint", "git add"]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
